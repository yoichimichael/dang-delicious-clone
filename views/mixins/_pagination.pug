mixin pagination(page, pagees, count)
  .pagination   
    .pagination__prev 
      if page > 1
        a(href=`/stores/page/${page - 1}`)
    .pagination__text 
      p Page #{page} of #{pages} - #{count} total results
    .pagination__next
      if page < pages 
        a(href=`/stores/pages/${parseFloat(page) + 1}`) Next